{
  "name": "NASA Image Search",
  "version": "0.0.1",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "manifest": "src/manifest.json",
    "main": "nasa_image_search.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.13",
    "eslint": "^5.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "lodash": ">=4.17.11"
  },
  "author": "Infinity Lab LLC",
  "repository": "https://github.com/BohemianCoding/unsplash-sketchplugin.git",
  "description": "Easily grab images from Unsplash",
  "license": "MIT",
  "dependencies": {
    "@skpm/fs": "^0.2.3"
  }
}
